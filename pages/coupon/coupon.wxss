page{
  background-color: #F0F0F0;
}
/* 优惠券 */
.outer{
  padding: 21rpx 29rpx;
  box-sizing: border-box;
  margin-bottom: 20rpx;
}
.one_discount_title{
  font-size: 26rpx;
  color: #232323;
  margin-bottom: 20rpx;
}
.one_discount{
  display: flex;
  justify-content: space-between;
  width: 100%;
  margin-bottom: 21rpx;
}
.discount_left{
  width: calc(100% - 100rpx);
  display: flex;
  position: relative;
}
.discount_only_image{
  position: absolute;
  width: 121.4rpx;
  height: auto;
  right: 0rpx;
  top: 50%;
  transform: translateY(-50%);
}
.discount_left_tou{
  width: 59.8rpx;
  height: 225rpx;
}
.discount_text{
  width: calc(100% - 59.8rpx);
  height: 225rpx;
  background-color: white;
  display: flex;
  justify-content: space-between;
}
.discount_left_text{
  width: 187rpx;
  padding-left: 20rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  position: relative;
  color: #1678FF;
}
.qitaColor{
  color: #FF6600;
}
.qitaColor1{
  color: #999999;
}
.qitaColor2{
  color: #FFB400;
}
.discount_left_text::after{
  content: "";
  display: inline-block;
  width: 3rpx;
  position: absolute;
  right: 0rpx;
  height: 156rpx;
  background-color: #DADADA;
}
.discount_left_text_top{
  width: 100rpx;
  font-size: 26rpx;
  line-height: 40rpx;
  text-align: center;
  border-radius: 10rpx;
}
.discount_left_text_color{
  border: 1rpx solid #FFB400;
}
.discount_left_text_color1{
  border: 1rpx solid #FF6600;
}
.discount_left_text_bottom{
  font-size: 50rpx;
  margin-left: -25rpx;
}
.discount_left_text_bottom text{
  font-size: 80rpx;
}
.discount_right_text{
  background-color: white;
  width: calc(100% - 187rpx);
  display: flex;
  flex-direction: column;
  justify-content: center;
  padding-left: 33rpx;
}
.discount_right_text_one{
  font-size: 26rpx;
  color: #232323;
  line-height: 50rpx;
}
.discount_right_text_two,.discount_right_text_three{
  font-size: 20rpx;
  color: #999999;
  line-height: 40rpx;
}
.clip_only_bottom .van-radio .van-radio__icon-wrap{
  height: 52rpx!important;
  background-color: rgba(0,0,0,0)!important;
  display: flex;
  align-items: center;
}
.van-radio{
  width: 52rpx;
  height: 100%;
}
.youhui-icon{
  padding-right: 20rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: flex-end;
}
.discount_radio{
  /* display: flex; */
  /* flex-direction: row-reverse; */
  /* padding-right: 20rpx; */
  width: 52rpx;
  height: 52rpx;
}
/* 右侧颜色文字 */
.discount_right_img{
  width: 100rpx;
  position: relative;
  font-size: 0rpx;
}
.discount_right_img image{
  width: 100%;
  height: 225rpx;
}
.discount_right_img text{
  writing-mode: tb-rl;
  position: absolute;
  display: inline-block;
  height: 130rpx;
  left: 50%;
  top: 50%;
  z-index: 999;
  color: white;
  font-size: 32rpx;
  transform: translateX(-50%) translateY(-50%);
}
/* 确定 */
.btn-bottom{
  font-size: 32rpx;
  line-height: 88rpx;
  text-align: center;
  width: calc(100% - 102rpx);
  background-color: #187AFF;
  border-radius: 88rpx;
  color: white;
  position: fixed;
  bottom: 20rpx;
  left: 50%;
  transform: translateX(-50%);
  z-index: 999;
}
/* 服务卡 */
.fuwuka{
  padding: 21rpx 29rpx;
  box-sizing: border-box;
  margin-bottom: 18rpx;
}
.fuwuka-top-title{
  font-size: 26rpx;
  color: #232323;
  margin-bottom: 20rpx;
}
.clip_only_outer{
  width: 100%;
  border-radius: 20rpx;
  color: white;
  padding: 30rpx 20rpx 7rpx;
  box-sizing: border-box;
  position: relative;
  box-shadow: 0rpx 0rpx 15rpx rgba(0,0,0,0.15);
  margin-bottom: 21rpx;
}
.img-is{
  position: absolute;
  top: 0rpx;
  left: 315rpx;
  width: 122rpx;
  height: auto;
}
/* 次卡顶部 */
.clip_only_top{
  display: flex;
  justify-content: space-between;
  border-bottom: 3rpx solid rgba(255,255,255,0.3);
}
/* 顶部左侧文字logo和文字 */
.clip_only_left{
  display: flex;
}
.clip_only_left .iconfont{
  font-size: 79rpx;
}
/* 文字 */
.clip_only_left_text{
  margin-left: 20rpx;
  display: flex;
  height: 79rpx;
  flex-direction: column;
  justify-content: space-between;
}
.clip_left_text_top{
  font-size: 32rpx;
}
.clip_left_text_bottom{
  font-size: 24rpx;
}
/* 右侧文字有效期 */
.clip_only_right{
  text-align: right;
  margin-bottom: 30rpx;
}
.clip_only_right_mon{
  font-size: 50rpx;
}
.clip_only_right_time{
  font-size: 24rpx;
}
/* 底部说明文字 */
.clip_only_bottom{
  padding-top:7rpx;
  display: flex;
  justify-content: space-between;
}
.clip_only_bottom_desc{
  font-size: 22rpx;
  line-height: 46rpx;
}
/* 背景图 */
.bg_image{
  position: absolute;
  width: 288rpx;
  height: auto;
  top: 50%;
  right: 0rpx;
  transform: translateY(-50%);
}
.van-radio__icon-wrap{
  background-color: white;
  border-radius: 50%;
  display: flex;
  align-items: center;
}
/* 不使用 */
.noUse{
  width: 100%;
  display: flex;
  justify-content: space-between;
  background-color: white;
  padding: 20rpx;
  box-sizing: border-box;
  border-radius: 10rpx;
  align-items: center;
  margin-bottom: 18rpx;
}
.noUse_text{
  font-size: 32rpx;
}
.fuwuka .noUse{
  background-color: #FF6600;
  color: white;
}
.fuwuka .van-radio__icon-wrap{
  background-color: rgba(0,0,0,0);
}