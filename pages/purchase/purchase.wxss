page{
  background-color: #F0F2F5;
}
.clip_outer{
  width: 100%;
  box-sizing: border-box;
  margin-top: 70rpx;
}
.clip_only_outer{
  width: calc(100% - 60rpx);
  margin-left: 30rpx;
  border-radius: 20rpx;
  color: white;
  padding: 30rpx 20rpx 7rpx;
  box-sizing: border-box;
  position: relative;
  margin-top: -70rpx;
  box-shadow: 0rpx 0rpx 15rpx rgba(0,0,0,0.15);
  z-index: 1;
}
/* 次卡顶部 */
.clip_only_top{
  display: flex;
  justify-content: space-between;
  border-bottom: 3rpx solid rgba(255,255,255,0.3);
}
/* 顶部左侧文字logo和文字 */
.clip_only_left{
  display: flex;
}
.clip_only_left .iconfont{
  font-size: 79rpx;
}
/* 文字 */
.clip_only_left_text{
  margin-left: 20rpx;
  display: flex;
  height: 79rpx;
  flex-direction: column;
  justify-content: space-between;
}
.clip_left_text_top{
  font-size: 32rpx;
}
.clip_left_text_bottom{
  font-size: 24rpx;
}
/* 右侧文字有效期 */
.clip_only_right{
  text-align: right;
  margin-bottom: 30rpx;
}
.clip_only_right_mon{
  font-size: 50rpx;
}
.clip_only_right_time{
  font-size: 24rpx;
}
/* 底部说明文字 */
.clip_only_bottom{
  padding-top:7rpx;
  display: flex;
  justify-content: space-between;
}
.clip_only_bottom_desc{
  font-size: 22rpx;
  line-height: 46rpx;
}
.clip_only_bottom_buy{
  background-color: white;
  height: 46rpx;
  width: 123rpx;
  font-size: 24rpx;
  line-height: 46rpx;
  text-align: center;
  border-radius: 46rpx;
  z-index: 1;
}
/* 背景图 */
.bg_image{
  position: absolute;
  width: 288rpx;
  height: auto;
  top: 50%;
  right: 0rpx;
  transform: translateY(-50%);
}
/* 顶部上箭头 */
.bottom-top-to{
  position: fixed;
  width: 100%;
  top: calc(100vh - 600rpx);
  text-align: center;
}
/* 规则 */
.guize{
  position: fixed;
  width: calc(100% - 60rpx);
  margin-left: 30rpx;
  top: 260rpx;
  height: calc(100vh - 900rpx);
  /* border: 1px solid blue; */
  /* overflow-x: hidden;
  overflow-y: scroll; */
  z-index: -10;
  font-size: 24rpx;
}
/* 暂不在服务区 */
.no-only{
  text-align: center;
  padding-top: 174rpx;
  background-color: white;
  height: 100%;
  box-sizing: border-box;
}
.no-image{
  width: 386rpx;
  height: auto;
}
.no-title{
  font-size: 30rpx;
  color: #666666;
  margin-top: 60rpx;
}
.van-popup .van-icon{
  font-size: 32rpx;
}
/* 购买方式 */
.buy-popup-title{
  text-align: center;
  font-size: 32rpx;
  line-height: 85rpx;
  position: relative;
  border-bottom: 1rpx solid #E4E4E4;
}
.buy-popup-title .iconfont{
  position: absolute;
  right: 30rpx;
  top: 0rpx;
}
.buy-popup-text-outer{
  padding: 10rpx 30rpx;
  font-size: 28rpx;
  color: #232323;
  line-height: 48rpx;
}
.buy-popup-btn{
  padding: 10rpx 30rpx 20rpx;
  display: flex;
  justify-content: space-between;
}
.buy-popup-btn-left,.buy-popup-btn-right{
  width: calc(50% - 10.5rpx);
  height: 80rpx;
  color: white;
  font-size: 30rpx;
  text-align: center;
  line-height: 80rpx;
  border-radius: 80rpx;
}
.buy-popup-btn-left{
  background-color: #FF6600;
}
.buy-popup-btn-left-typ{
  background-color: #999999;
  width: calc(50% - 10.5rpx);
  height: 80rpx;
  color: white;
  font-size: 30rpx;
  text-align: center;
  line-height: 80rpx;
  border-radius: 80rpx;
}
.buy-popup-btn-right{
  background-color: #22AC38;
}
/* 填写车辆信息 */
.add-car-title{
  color: #232323;
  font-size: 26rpx;
  line-height: 70rpx;
  text-align: center;
  font-weight: bold;
  border-bottom: 1rpx solid #DBDBDB;
  position: relative;
}
.add-car-title .iconfont{
  position: absolute;
  line-height: 70rpx;
  right: 20rpx;
  top: 0rpx;
}
.add-car-text{
  padding: 20rpx;
  box-sizing: border-box;
  font-size: 26rpx;
  color: #FE0000;
  display: flex;
}
.add-car-text text{
  margin-left: 15rpx;
}
.add-car-btn-one{
  padding: 0rpx 20rpx 20rpx;
  box-sizing: border-box;
}
.add-car-btn{
  width: 100%;
  font-size: 30rpx;
  color: white;
  line-height: 80rpx;
  border-radius: 80rpx;
  text-align: center;
  background-color: #FF6600;
}
/* 提交车辆信息 */
.add-car-submit{
  width: calc(100% - 40rpx);
  line-height: 80rpx;
  font-size: 32rpx;
  color: white;
  background-color: #1678FF;
  text-align: center;
  border-radius: 80rpx;
  margin: 30rpx 20rpx;
}
/* 车牌键盘 */
/* 车牌号 */
.city,.openCarNumber { 
  background: #eeeeee ; 
  overflow: hidden ; 
  position: absolute ; 
  bottom: 0 ; 
  z-index: 999; 
  display: block ; 
  padding : 20rpx 0 10rpx 0 ;
  left: 0rpx;
  right: 0rpx
}
.cityItem { 
  width: 10% ; 
  height : 70rpx ; 
  line-height: 70rpx ; 
  background: #fff ; 
  margin : 15rpx 1.25% ; 
  float: left; 
  box-shadow:0 2px 4px 0 rgba(123,123,123,0.50); 
  border-radius: 6px ; 
  font-size: 36rpx ; 
  text-align: center ;
} 
.del {
  font-size: 28rpx ;
  background: #1678FF ; 
  color : #fff ; 
  width : 15% ;
} 
.hide{
  display: none;
}
/* 支付弹窗 */
.tip{
  text-align: center;
  font-size: 30rpx;
  color: #232323;
  line-height: 88rpx;
  border-bottom: 1rpx solid #E1E1E1;
  position: relative;
}
.tip .iconfont{
  position: absolute;
  top: 0rpx;
  right: 20rpx;
}
.password-money{
  text-align: center;
  color: #1678FF;
  font-weight: bold;
  font-size: 50rpx;
  line-height: 117rpx;
}
.btn-bottom{
  width: calc(100% - 40rpx);
  margin-left: 20rpx;
  font-size: 32rpx;
  color: white;
  line-height: 88rpx;
  background-color: #1678FF;
  text-align: center;
  border-radius: 88rpx;
  margin-bottom: 40rpx;
}
.closePopup{
  position: absolute;
  top: 0rpx;
  right: 20rpx;
  line-height: 70rpx;
  font-size: 32rpx;
}