/* 车牌号 */
.city,.openCarNumber { 
  background: #eeeeee ; 
  overflow: hidden ; 
  position: absolute ; 
  bottom: 0 ; 
  z-index: 999; 
  display: block ; 
  padding : 20rpx 0 10rpx 0 ;
  left: 0rpx;
  right: 0rpx
}
.cityItem { 
  width: 10% ; 
  height : 70rpx ; 
  line-height: 70rpx ; 
  background: #fff ; 
  margin : 15rpx 1.25% ; 
  float: left; 
  box-shadow:0 2px 4px 0 rgba(123,123,123,0.50); 
  border-radius: 6px ; 
  font-size: 36rpx ; 
  text-align: center ;
} 
.del {
  font-size: 28rpx ;
  background: #1678FF ; 
  color : #fff ; 
  width : 15% ;
} 
.hide{
  display: none;
}
