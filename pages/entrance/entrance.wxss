  margin-top: 24rpx;
  color: white;
}
/* 头像/欢迎文字 */

.header_top_three{
  display: flex;
  justify-content: space-around;
  margin-top: 25rpx;
  align-items: center;
}
.header_top_only{
  color: white;
  text-align: center;
  width: 100%;
}
.header_border{
  width: 6rpx;
  height: 58rpx;
  background-color: white;
  /* border-left: 0.5rpx solid white; */
}
.header_top_num{
  font-size: 36rpx;
}
.header_top_bottom_text{
  font-size: 28rpx;
}
/* 汽车精选/汽车保养 */
.car_two_outer{
  padding: 24rpx;
  display: flex;
  justify-content: space-between;
}
.car_left,.car_right{
  width: calc(50% - 45rpx);
  height: 148rpx;
  box-shadow: 0rpx 0rpx 10rpx rgba(0,0,0,0.2);
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0rpx 13rpx 0rpx 20rpx;
  border-radius: 10rpx;
  background-color: white;
  position: relative;
}
.car_left image,.car_right image{
  position: absolute;
  right: 0rpx;
  top: 0rpx;
  width: 168rpx;
  height: auto;
}
.right_text_car .text_car_one,.right_right_text_car .text_car_one{
  font-size: 40rpx;
  font-weight: bold;
  color: #232323;
}
.right_text_car .text_car_two,.right_right_text_car .text_car_two{
  font-size: 22rpx;
  letter-spacing: 5rpx;
  color: #666666;
}
.right_text_car .iconfont,.right_right_text_car .iconfont{
  font-size: 26rpx;
  margin-top: 5rpx;
}
/* 现在预约 */
.subscribe_outer{
  margin: 0rpx 24rpx 24rpx;
  background-color: white;
  box-shadow: 0rpx 0rpx 10rpx rgba(0,0,0,0.2);
  border-radius: 10rpx;
}
/* 现在预约顶部 */
.subscribe_top{
  font-size: 26rpx;
  line-height: 64rpx;
  padding: 0rpx 20rpx;
  box-sizing: border-box;
  border-bottom: 1rpx solid #E1E1E1;
  color: #666666;
  display: flex;
  justify-content: space-between;
}
.subscribe_top .left_subscribe_top{
  display: flex;
  align-items: center;
}
.weather_top{
  line-height: 64rpx;
}
.subscribe_top_weather{
  display: flex;
  line-height: 64rpx;
}
.yuanCheng{
  display: inline-block;
  width: 9rpx;
  height: 9rpx;
  border-radius: 9rpx;
  margin: 0rpx 5rpx;
}
.right_weather_text,.subscribe_top_right_out{
  display: flex;
  align-items: center;
  height: 100%;
}
/* 爱车信息 */
.subscribe_center{
  margin: 30rpx 24rpx 0rpx;
  padding-bottom: 22rpx;
  border-bottom: 1px solid #E1E1E1;
}
.no-car{
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.subscribe_center_left{
  display: flex;
  align-items: center;
}
.subscribe_center_right view{
  font-size: 35rpx;
  color: #666666;
}
.yes-car{
  width: 100%;
  display: flex;
  justify-content: space-between;
  position: relative;
}
.yes-car-left{
  width: calc(100% - 32rpx);
  display: flex;
  align-items: center;
}
.yes-car-left image{
  width: 80rpx;
  height: auto;
  margin-right: 20rpx;
}
.yes-car-left-text{
  width: calc(100% - 100rpx);
  font-size: 40rpx;
}
.yes-car-info{
  font-size: 28rpx;
  color: #666666;
}
.yes-car-right{
  position: absolute;
  right: 0rpx;
  top: 50%;
  transform: translateY(-50%);
  display: flex;
  align-items: center;
  justify-content: flex-end;
}
.subscribe_center_right_outer{
  display: flex;
  width: 239rpx;
  align-items: center;
  justify-content: flex-end;
}
.yes-car-right image,.subscribe_center_right_outer image{
  width: 188rpx;
  height: auto;
  margin-right: 10rpx;
}
/* 当前位置 */
.address_center{
  padding: 31rpx 24rpx 24rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  font-size: 28rpx;
  color: #FF6600;
}
.address_center text{
  display: inline-block;
}
.leftAddress{
  width: 150rpx;
}
.rightAddress{
  width: calc(100% - 150rpx);
  color: #232323;
}
.blur_address_center{
  color: #1678FF;
}
.address_center .icon-you{
  display: flex;
  align-items: center;
}
