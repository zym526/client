/* 优惠券 */
.discounts_outer,.random_outer{
  position: fixed;
  z-index: 999;
  left: 0rpx;
  right: 0rpx;
  top: 0rpx;
  bottom: 0rpx;
  background-color: rgba(0,0,0,0.5);
}
.outer_bian{
  position: relative;
  margin-top: 15%;
}
.random_outer .outer_bian{
  margin-top: 20%;
}
.discounts_inside{
  width: 80%;
  background-color: white;
  margin-left: 10%;
  border-radius: 20rpx;
  text-align: center;
  padding: 40rpx 0rpx 50rpx;
  position: relative;
}
.discounts_inside .random-text{
  font-size: 80rpx;
  font-weight: bolder;
  color: #7C4810;
  position: absolute;
  left: calc(45% + 5rpx);
  top: 240rpx;
}
.inside-text{
  font-size: 26rpx;
  color: #666;
}
.inside-text text{
  color: #FA6649;
}
.packetTop{
  position: absolute;
  left: 50%;
  top: -86rpx;
  transform: translateX(-50%);
}
.redPacket{
  margin-top: 84rpx!important;
}
.discounts_inside_title{
  font-size: 46rpx;
}
.discounts_inside_look{
  font-size: 32rpx;
  margin-top: 15rpx;
}
.discounts_inside_look text{
  color: #1678FF;
}
.discounts_inside image{
  width: 90%;
  height: auto;
  margin-top: 40rpx;
}
.discounts_inside button{
  width: 411rpx;
  font-size: 32rpx;
  line-height: 88rpx;
  color: white;
  background-color: #1678FF;
  margin-top: 45rpx;
}
.cross_outer{
  display: inline-block;
  width: 60rpx;
  height: 60rpx;
  border-radius: 50%;
  border: 4rpx solid white;
  display: flex;
  align-items: center;
  justify-content: center;
  position: absolute;
  left: 50%;
  bottom: -90rpx;
  transform: translateX(-50%);
}
.cross_random{
  border-radius: 60rpx;
  background:-moz-linear-gradient(left,#FDEDA0 25%,#E3CC5A);
  background:-webkit-linear-gradient(left,#FDEDA0 25%,#E3CC5A);
  background:-o-linear-gradient(left,#FDEDA0 25%,#E3CC5A);
  background:-ms-linear-gradient(left,#FDEDA0 25%,#E3CC5A);
  background:linear-gradient(left,#FDEDA0 25%,#E3CC5A);
  color: #79460C!important;
}
